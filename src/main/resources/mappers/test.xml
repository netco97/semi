<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.HumanMapper">
  <select id="getHuman" resultType="Human">
    select * from human_test where h_name = #{val}
  </select>
  <select id="getAllHuman" resultType="Human" >
    select * from human_test
  </select>
  <insert id="regHuman">
	  insert into human_test values(#{h_name}, #{h_age})
  </insert>
</mapper>